{
  "name": "portal-ui-ng",
  "version": "0.3.0",
  "scripts": {
    "build:schematics": "tsc -p tsconfig.schematics.json",
    "postbuild:schematics": "copyfiles schematics/*/schema.json schematics/*/files/** schematics/collection.json ../../dist/portal-ui-ng/"
  },
  "schematics": "./schematics/collection.json",
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@types/lodash-es": ">=4.17.0"
  },
  "peerDependencies": {
    "@angular/animations": "~21.0.0",
    "@angular/cdk": "~21.0.0",
    "@angular/common": "~21.0.0",
    "@angular/core": "~21.0.0",
    "@angular/forms": "~21.0.0",
    "@angular/router": "~21.0.0",
    "date-fns": "^4.1.0",
    "humanize-duration": "^3.33.0",
    "lodash-es": ">=4.17.0",
    "tailwindcss": "^4.1.0",
    "vanilla-calendar-pro": "^3.0.1"
  },
  "peerDependenciesMeta": {
    "humanize-duration": {
      "optional": true
    },
    "vanilla-calendar-pro": {
      "optional": true
    }
  },
  "exports": {
    "./assets/base/_index.css": {
      "default": "./assets/base/_index.css"
    },
    "./assets/components/_index.css": {
      "default": "./assets/components/_index.css"
    },
    "./assets/pages/_index.css": {
      "default": "./assets/pages/_index.css"
    }
  },
  "sideEffects": false
}
